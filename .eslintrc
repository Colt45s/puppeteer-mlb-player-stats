{
  "parser": "@typescript-eslint/parser",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": [
    "eslint:recommended",
    "prettier/@typescript-eslint"
  ],
  "plugins": [
    "@typescript-eslint",
    "prettier"
  ],
  "parserOptions": {
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "prettier/prettier": 2,
    "@typescript-eslint/adjacent-overload-signatures": 2,
    "no-unused-vars": 0,
    "@typescript-eslint/no-unused-vars": 2,
    "no-console": 0
  }
}